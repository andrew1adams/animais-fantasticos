{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["airbnb-base"],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    // "no-console": 0
    "no-param-reassign": 0,
    "semi": 0,
    "import/prefer-default-export": 0,
    "import/extensions": 0,
    "max-classes-per-file": 0
  }
}
